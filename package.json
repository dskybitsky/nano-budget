    "@mantine/modals": "8.3.2",
    "@next/bundle-analyzer": "^15.3.3",
    "@prisma/client": "6.16.2",
    "@tabler/icons-react": "^3.34.0",
    "dayjs": "^1.11.18",
    "lodash": "^4.17.21",
    "mantine-form-zod-resolver": "^1.3.0",
    "moment": "^2.30.1",
    "next": "15.5.7",
    "next-auth": "^5.0.0-beta.29",
    "next-intl": "^4.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
